{
  "compilerOptions": {
    "strictNullChecks": true,
    "skipLibCheck": true,
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "allowJs": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "outDir": "out",
    "rootDir": "src",
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"],
  "sourceMap": true
}
